<?php

/**
 * @file
 * Contains rusa_perm_results.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function rusa_perm_results_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the rusa_perm_results module.
    case 'help.page.rusa_perm_results':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Store ride results for Permanents') . '</p>';
      return $output;

    default:
  }
}
